/* pages/search/search.wxss */
@import "../../static/css/common.wxss";

.page {   
	width: 100%;   
	height: 100%;  
} 
.searchArea {
	padding: 40rpx 60rpx;
	text-align: center;
}
.searchInput {
	width: 500rpx;
	height: 60rpx;
	border-radius: 100rpx;
	color: rgba(136, 136, 136, 1);
	text-align: left;
	font-family: Roboto;
	border: 1px solid rgba(187, 187, 187, 1);
	padding-left: 20rpx;
}
.searchBtn {
	width: 100rpx;
	height: 60rpx;
	color: rgba(16, 16, 16, 1);
	text-align: center;

}
.btnArea {
	width: 100%;
	text-align: center;
}
.histArea {
	padding: 40rpx 40rpx 40rpx 60rpx;
}
.keyItem {
	margin: 0rpx 40rpx 0rpx 60rpx;
	border-bottom: 1px solid #EEEEEE;
	padding: 20rpx 0rpx;
}
.firstItem {
	border-top: 1px solid #EEEEEE;
}
.btnArea {
	margin-top: 60rpx;
}
.histBtn {
	line-height: 60rpx;
	border-radius: 100rpx;
	text-align: center;
	font-family: Roboto;
	border: 1px solid rgba(187, 187, 187, 1);
	padding: 15rpx 100rpx;
}

.qitem {
  padding: 30rpx 20rpx 20rpx;
  border-bottom: 1rpx solid rgba(229,229,229,1);
}
.lowerBar {
	margin-top: 10rpx;
}
.labelsq {
	margin-right: 20rpx;
	padding: 10rpx 20rpx 0rpx;
	height: 40rpx;
	line-height: 29rpx;
	background-color: rgba(226, 226, 226, 1);
	color: rgba(16, 16, 16, 1);
	text-align: center;

}

