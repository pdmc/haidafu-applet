@import "../../static/css/common.wxss";
.container {
  background-color: #F8F8F8;
  height: 100%;
  width:100%;
  font-size: 32rpx;
  /*line-height: 1.6;*/
}
.container .sw {
  width: 100%;
	height: 350rpx;
}
.page-body{
  width: 100%;
  margin-top: 10rpx;
  padding: 0rpx 0rpx;
}
.page-body .sw {
  width: 100%;
  text-align: center;
}
.swiper-item{
  text-align: center;
  width: 100%;
	/*height: 1;*/
  overflow:show;
}
.slide-image {
  width:100%;
	height: 375rpx;
  overflow:show;
}
.search_section {
  text-align: center;
  align-items:center;/*垂直居中*/
  justify-content: center;/*水平居中*/
  margin: 0rpx 0rpx;
  padding: 0rpx 0rpx;
}
.search_section > image {
  width: 700rpx;
  height: 150rpx;
  margin: 0rpx 0rpx;
}

.ai_section {
	margin: 0rpx 30rpx 30rpx;
}
.ai_section image {
	width: 333rpx;
	height: 125rpx;
}

.select-header {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;  
  align-items: center;
  padding: 0rpx 0rpx;
  margin: 0rpx 30rpx;
}
.img-select {
  height: 60rpx;
  width: 60rpx;
}
.select-title {
  font-size: 42rpx;
  color: #101010;
  margin-left: 15rpx;
  font-family: 方正兰亭黑-标准;
}
.select-tab{
	display: inline-block;
	width: 100%;
	margin: 0rpx 20rpx;
  font-size: 32rpx;
	white-space: nowrap;
	overflow-x:scroll;
  /*
	height: 130rpx;
	display: flex;
  flex-direction: row;
  align-items: stretch;
  justify-content: space-around;
  overflow-x: show;
	display: flex;
  flex-direction: row;
  align-items: stretch;
  justify-content: space-around;
	background-color: #fff;
  border-top:1px solid #FFFFFF;
  border-bottom:1px solid #FFFFFF;*/
}
.select-tab > view{
	display: inline-block;
	line-height: 40rpx;
	margin: 30rpx 20rpx;
  padding: 24rpx 0rpx;
	text-align: center;
	overflow: hidden;
	text-overflow: ellipsis;

  /*display: flex;
  align-items: center;
  margin: 30rpx 0rpx;
  padding: 24rpx 0rpx;*/
}
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}
.select-tab > view.inactive {
  border-bottom: 3rpx solid #FFFFFF;
}
.select-tab > view.active{
  color: #1FAFFF;
  border-bottom: 3rpx solid #1FAFFF;
}
.select-swiper {
  height: 350rpx;
  min-height: 350rpx;
}
.select-swiper-item {
  margin: 0rpx 30rpx;
}
.select-swiper-view {
  border-radius: 10rpx;
	margin-right: 30rpx;
	width: 690rpx;
}
.select-text-high {
  font-size: 36rpx;
  color: #FFFFFF;
  font-family: 方正兰亭黑-标准;
  padding: 200rpx 0rpx 0rpx 30rpx;
	overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.select-text-low {
  color: #FFFFFF;
  font-family: 方正兰亭黑-标准;
  padding: 0rpx 20rpx 10rpx 30rpx;
}
.select-text-price{
  font-size: 36rpx;
  margin-right: 20rpx;
}
.select-text-sq{
  font-size: 32rpx;
}
.select-text-country{
  font-size: 28rpx;
}
